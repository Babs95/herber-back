{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/back-end-herbersn/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/back-end-herbersn/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/back-end-herbersn",
  "projects": {
    "back-end-herbersn": {
      "type": "application",
      "root": "apps/back-end-herbersn",
      "entryFile": "main",
      "sourceRoot": "apps/back-end-herbersn/src",
      "compilerOptions": {
        "tsConfigPath": "apps/back-end-herbersn/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "prisma-service": {
      "type": "library",
      "root": "libs/prisma-service",
      "entryFile": "index",
      "sourceRoot": "libs/prisma-service/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma-service/tsconfig.lib.json"
      }
    },
    "monitoring-service": {
      "type": "application",
      "root": "apps/monitoring-service",
      "entryFile": "main",
      "sourceRoot": "apps/monitoring-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monitoring-service/tsconfig.app.json"
      }
    },
    "logging": {
      "type": "library",
      "root": "libs/logging",
      "entryFile": "index",
      "sourceRoot": "libs/logging/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logging/tsconfig.lib.json"
      }
    }
  }
}
